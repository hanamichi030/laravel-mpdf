{
  "name": "carlos-meneses/laravel-mpdf",
  "description": "Laravel Mpdf: Using Mpdf in Laravel to generate Pdfs.",
  "keywords": [
    "mpdf",
    "pdf",
    "laravel"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/hanamichi030/mpdf.git",
      "branch": "feature/only-first-page"
    }
  ],
  "require": {
    "php": "^5.6|^7.0|^8.0",
    "hanamichi030/mpdf": "dev-feature/only-first-page"
  },
  "autoload": {
    "psr-4": {
      "Mccarlosen\\LaravelMpdf\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mccarlosen\\LaravelMpdf\\LaravelMpdfServiceProvider"
      ],
      "aliases": {
        "PDF": "Mccarlosen\\LaravelMpdf\\Facades\\LaravelMpdf"
      }
    }
  }
}
